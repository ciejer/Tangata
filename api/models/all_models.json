{
    "version": 0.1,
    "models": [
        {
            "name": "humans",
            "description": "Human Beings, from planet Earth",
            "file_name": "humans.sql",
            "columns": [
                "human_id",
                "name",
                "favourite_colour",
                "tasting_notes"
            ]
        },
        {
            "name": "abductions",
            "description": "Attempts to abduct a human for 'testing'",
            "file_name": "abductions.sql",
            "columns": [
                "human_id",
                "abduction_datetime",
                "location_universe_grid",
                "successful_flag",
                "survived_flag"
            ],
            "joinConditions": [{
                "conditionField1": {"model": "abductions", "column": "human_id"},
                "conditionOperator": "=",
                "conditionField2": {"model": "humans", "column": "human_id"},
                "fullName": "abductions.human_id = humans.human_id"
            }]
        }
    ],
    "conditions": [
        "humans.favourite_colour = 'Green'"
    ]
}